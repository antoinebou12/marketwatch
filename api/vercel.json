{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": {
        "buildCommand": "pip install -r requirements.txt && pip install --upgrade pip",
        "debug": true,
        "excludeFiles": "docs/**, test/**, *test.py, .github/**,requirements.txt, SECURITY.md, .pre-commit.config.yaml, renovate.json, .gitignore, .gitattributes, .git"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ]
}

